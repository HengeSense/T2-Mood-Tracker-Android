<resources>
	<style name="VASSeekBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_bar_horizontal</item>
        <item name="android:indeterminateDrawable">@drawable/progress_bar_horizontal</item>
        <item name="android:minHeight">50dip</item>
